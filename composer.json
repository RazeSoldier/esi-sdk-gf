{
    "name": "razesoldier/esi-sdk-gf",
    "type": "library",
    "description": "A SDK that used to access ESI of Serenity server",
    "keywords": ["eveonline"],
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^7.4 || ^8.0",
        "psr/http-client": "^1",
        "symfony/http-client": "^5.2",
        "nyholm/psr7": "^1.4",
        "netresearch/jsonmapper": "^4.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "RazeSoldier\\SerenityEsi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
